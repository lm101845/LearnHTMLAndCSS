		'focus' : function(){
			var $this = $(this);
			var $parent = $this.parent();
			var $label = $parent.find('.debLogin-label');
			$label.hide();
		},
		'blur'  : function(){
			var $this = $(this);
			var vlue = $.trim( $this.val() ); 
